{
  "rewrites": [
    { "source": "/about", "destination": "/about.html" },
    { "source": "/team", "destination": "/team.html" },
    { "source": "/team-single", "destination": "/team-single.html" },
    { "source": "/single-service", "destination": "/single-service.html" },
    { "source": "/services", "destination": "/services.html" },
    { "source": "/projects", "destination": "/projects.html" },
    { "source": "/projects-details", "destination": "/projects-details.html" },
    { "source": "/faq", "destination": "/faq.html" },
    { "source": "/contact", "destination": "/contact.html" }
  ],
  "headers": [
    {
      "source": "/(.*)\\.(png|jpg|jpeg|webp|svg|gif|ico)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "cleanUrls": true
}
